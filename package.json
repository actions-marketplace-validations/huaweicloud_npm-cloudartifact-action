{
  "name": "npm-cloudartifact-action",
  "version": "v1.1.0",
  "description": "Action for uploading or downloading artifact from Huawei Cloud npm CloudArtifact",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "package": "ncc build -m --license licenses.txt",
    "lint-create": "eslint --init",
    "lint": "eslint src",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest",
    "all": "npm run test && npm run lint && npm run format && npm run build && npm run package"
  },
  "keywords": [
    "npm",
    "cloudartifact"
  ],
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@actions/core": "^1.8.2"
  }
}
